    <!-- http://www.bbc.co.uk/ontologies/sport/#subDiscipline -->

    <owl:ObjectProperty rdf:about="http://www.bbc.co.uk/ontologies/sport/#subDiscipline">
        <owl:inverseOf rdf:resource="http://www.bbc.co.uk/ontologies/sport/#subDisciplineOf"/>
        <rdfs:domain rdf:resource="http://www.bbc.co.uk/ontologies/sport/SportsDiscipline"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#Discipline"/>
        <rdfs:comment>Relates a discipline with a child discipline, for example gymnastics with rhythmic gymnastics.</rdfs:comment>
        <rdfs:label xml:lang="en">subDiscipline</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.bbc.co.uk/ontologies/sport/#subDisciplineOf -->

    <owl:ObjectProperty rdf:about="http://www.bbc.co.uk/ontologies/sport/#subDisciplineOf">
        <rdfs:comment>Relates a discipline with a parent discipline, for example rhythmic gymnastics with gymnastics.</rdfs:comment>
        <rdfs:label xml:lang="en">subDisciplineOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#GamesTypeSport -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#GamesTypeSport">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#SportCompetition"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/sport/SportsDiscipline"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#Discipline"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A competition is of a type of sport discipline</rdfs:comment>
        <rdfs:label xml:lang="en">gamesTypeSport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignBronze -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignBronze">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#SportCompetition"/>
        <rdfs:range rdf:resource="http://www.bbc.co.uk/ontologies/sport/MedalCompetition"/>
        <rdfs:comment>Relationship between SportCompetition and Bronze Medal</rdfs:comment>
        <rdfs:label>assignBronze</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignGold -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignGold">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#SportCompetition"/>
        <rdfs:range rdf:resource="http://www.bbc.co.uk/ontologies/sport/MedalCompetition"/>
        <rdfs:comment>Relationship between SportCompetition and Gold Medal</rdfs:comment>
        <rdfs:label>assignGold</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignSilver -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignSilver">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#SportCompetition"/>
        <rdfs:range rdf:resource="http://www.bbc.co.uk/ontologies/sport/MedalCompetition"/>
        <rdfs:comment>Relationship between SportCompetition and Silver Medal</rdfs:comment>
        <rdfs:label>assignSilver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#athleteOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#athleteOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPart"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#hasAthlete"/>
        <rdfs:comment xml:lang="en">Athlete is a player of a Team</rdfs:comment>
        <rdfs:label xml:lang="en">athleteOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_bronze -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_bronze">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#winAward"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#thrPlace"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignBronze"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Athlete and Bronze Medal</rdfs:comment>
        <rdfs:label xml:lang="en">award_bronze</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_gold -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_gold">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#winAward"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#fistPlace"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignGold"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Athlete and Gold Medal</rdfs:comment>
        <rdfs:label xml:lang="en">award_gold</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_silver -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#award_silver">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#winAward"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#secondPlace"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#assignSilver"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Athlete and Silver Medal</rdfs:comment>
        <rdfs:label xml:lang="en">award_silver</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#compose -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#compose">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPart"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Team composes by n people: doctor, athlete, trainers etc.</rdfs:comment>
        <rdfs:label xml:lang="en">compose</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#contains -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#contains">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsVenue"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCity"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCountry"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Venue, City, Country and Continent</rdfs:comment>
        <rdfs:label xml:lang="en">contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCity">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCountry"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Relationship between County and City</rdfs:comment>
        <rdfs:label xml:lang="en">containsCity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsCountry">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInContinent"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Relationship between Continent and Country</rdfs:comment>
        <rdfs:label xml:lang="en">containsCountry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsVenue -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#containsVenue">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCity"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Relationship between City and Venue</rdfs:comment>
        <rdfs:label xml:lang="en">containsVenue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#fistPlace -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#fistPlace">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#podium"/>
        <rdfs:comment xml:lang="en">first Place</rdfs:comment>
        <rdfs:label xml:lang="en">firstPlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#hasAthlete -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#hasAthlete">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#compose"/>
        <rdfs:comment xml:lang="en">A team has one or more athlete</rdfs:comment>
        <rdfs:label xml:lang="en">hasAthlete</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#haveMatch -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#haveMatch">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfDiscipline"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Relationship between SportCompetition and Discipline</rdfs:comment>
        <rdfs:label xml:lang="en">haveMatch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#hostCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#hostCountry">
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Country"/>
        <rdfs:comment xml:lang="en">Host country of an Olympiad</rdfs:comment>
        <rdfs:label xml:lang="en">hostCountry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPart -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPart">
        <rdfs:comment xml:lang="en">Person is part of a team with a specific role</rdfs:comment>
        <rdfs:label xml:lang="en">isPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPlayedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#isPlayedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#play"/>
        <rdfs:comment xml:lang="en">A sport competition is played by two or more teams</rdfs:comment>
        <rdfs:label xml:lang="en">isPlayedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCity"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCountry"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInContinent"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Continent,  Country , City and Venue</rdfs:comment>
        <rdfs:label xml:lang="en">locatedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.bbc.co.uk/ontologies/sport/#Venue"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#City"/>
        <rdfs:comment xml:lang="en">Relationship between Venue and City</rdfs:comment>
        <rdfs:label xml:lang="en">locatedInCity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInContinent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInContinent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Country"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Continent"/>
        <rdfs:comment xml:lang="en">Relationship between Country and Continent</rdfs:comment>
        <rdfs:label xml:lang="en">locatedInContinent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#locatedInCountry">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#City"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Country"/>
        <rdfs:comment xml:lang="en">Relationship between City and Country</rdfs:comment>
        <rdfs:label xml:lang="en">locatedInCountry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfDiscipline -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfDiscipline">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#SportCompetition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#Discipline"/>
        <rdfs:comment xml:lang="en">Relationship between Discipline and SportCompetition</rdfs:comment>
        <rdfs:label xml:lang="en">matchOfDiscipline</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfSport -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfSport">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#matchOfDiscipline"/>
            <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/sport/#subDisciplineOf"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relationship between Sport and SportCompetition</rdfs:comment>
        <rdfs:label xml:lang="en">matchOfSport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#nationality -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#nationality">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#CountryTeam"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Country"/>
        <rdfs:comment xml:lang="en">Relationship between Team and Country.</rdfs:comment>
        <rdfs:label xml:lang="en">nationality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#organize -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi#organize">
        <rdfs:comment>An Organizer organizes an Event</rdfs:comment>
        <rdfs:label>organize</rdfs:label>
