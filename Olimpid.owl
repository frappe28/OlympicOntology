        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <dc:date>2020-07-24</dc:date>
        <dc:description xml:lang="en">Ontology for the representation and description of entities related to the world of the Olympics such as editions, Olympic disciplines, medals and awards, athletes, etc.</dc:description>
        <dc:title xml:lang="en">Olympic Ontology</dc:title>
        <rdfs:label xml:lang="en">OlympicOntology</rdfs:label>
        <voc:PreferredNameSpacePrefix>olyo</voc:PreferredNameSpacePrefix>
        <voc:PreferredNameSpaceUri>http://www.semanticweb.org/francesca/ontologies/2020/6/olimpiadi</voc:PreferredNameSpaceUri>
    </owl:Ontology>
    


